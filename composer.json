{
  "name": "honey-comb/scripts",
  "description": "HoneyComb scripts",
  "keywords": ["honeycomb", "honey", "laravel", "cms"],
  "homepage": "https://github.com/honey-comb/scripts/",
  "license": "MIT",
  "authors": [
    {
      "name": "Mantas Paulauskas",
      "email": "mp@interactivesolutions.lt"
    },
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.5.*",
    "honey-comb/starter": "^0.1.0"
  },
  "require-dev": {
    "brianium/paratest": "^1.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench-browser-kit": "^3.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "HoneyComb\\Scripts\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HoneyComb\\Scripts\\Providers\\HCScriptsServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
